{
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "insertPragma": false,
    "jsxSingleQuote": true,
    "printWidth": 120,
    "requirePragma": false,
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false,
    "importOrder": ["react", "^react", "<THIRD_PARTY_MODULES>", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
